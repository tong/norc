{
	"name" : "Norc",
	"description" : "Network Organism Responsible for Communication",
	"version" : "0.1.1",
	"offline_enabled": true,
	"app" : {
		"background" : {
			"scripts" : ["background.js"]
		}
	},
	"permissions": [
		"alarms",
		"experimental",
		"notifications",
		"storage",
		"unlimitedStorage",
		"audioCapture",
		"videoCapture",
		"http://*/*", "https://*/*",
		{
			"socket": [
				"tcp-connect:*:5222",
				"tcp-connect:*:5223"
			]
		}
	],
	"icons": {
		"16": "icon/norc_16.png",
		"32": "icon/norc_32.png",
		"48": "icon/norc_48.png",
		"128": "icon/norc_128.png"
	}
}
