{
	"name": "Norc extension",
	"version": "0.1.1",
	"manifest_version": 2,
	"description": "Network organism responsible for communication",
  	"minimum_chrome_version": "31",
  	"offline_enabled": true,
	"background": {
		"scripts" : ["norc.js"]
	},
	"browser_action": {
		"default_icon": {
			"19": "icon/norc_32.png",
			"38": "icon/norc_48.png"
		},
		"default_title": "Norc",
		"default_popup": "popup.html"
	},
	"omnibox": { "keyword" : "norc" },
	"options_page": "settings.html",
	"icons": {
		"16": "icon/norc_16.png",
		"32": "icon/norc_32.png",
		"48": "icon/norc_48.png",
		"128": "icon/norc_128.png"
	},
	"permissions": [
		"history",
		"tabs",
		"idle",
		"bookmarks",
		"notifications",
		"contextMenus",
		"downloads",
		"storage",
		"unlimitedStorage",
		"http://*/*", "https://*/*"
	],
	"web_accessible_resources": [
		"icon/*.png"
	]
}
